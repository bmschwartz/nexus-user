generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @default(autoincrement()) @id
  password  String
  email     String
  username  String   @unique
  admin     Boolean? @default(false)
  createdAt DateTime @default(now())
}
